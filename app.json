{
  "expo": {
    "name": "WeldConnect - Mobile Welding Network",
    "slug": "weldconnect",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "WeldConnect is the premier mobile welding services marketplace connecting customers with certified professional welders and metal fabrication specialists. Find portable welding, on-site welding services, emergency welding repairs, custom metal fabrication, steel fabrication, and industrial welding services near you. Post welding jobs, receive competitive bids from local welding contractors, and hire the best welder for your project - all in one easy-to-use platform.",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fast.weldingservice",
      "keywords": [
        "welding",
        "welding services",
        "mobile welder",
        "portable welding",
        "welder near me",
        "metal fabrication",
        "steel fabrication",
        "on-site welding",
        "emergency welding",
        "welding contractor",
        "welding jobs",
        "metal work",
        "fabrication services",
        "professional welder",
        "welding quotes",
        "welding bids",
        "hire welder",
        "custom fabrication",
        "industrial welding",
        "mobile welding",
        "welding repair",
        "certified welder",
        "local welder",
        "welding marketplace",
        "metal repair"
      ]
    },
    "android": {
      "package": "fast.weldingservice",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#007AFF"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
